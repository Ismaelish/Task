@model RFS
@{
    Layout = "_Layout";
}
@{
    ViewData["Title"] = "Details";
}

<h1>Transaction Details</h1>

<div>
    <h4>@Html.DisplayFor(model => model.Reference)</h4>
    <hr />
    <dl class="row">
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.Reference)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Reference)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.ReferenceDate)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.ReferenceDate)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.TranCompNo)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.TranCompNo)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.Section)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Section)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.Branch)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Branch)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.Dept)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Dept)
        </dd>

    </dl>
</div>
<div>
    <button type="submit" value="submit" class="btn btn-primary">Submit</button>
